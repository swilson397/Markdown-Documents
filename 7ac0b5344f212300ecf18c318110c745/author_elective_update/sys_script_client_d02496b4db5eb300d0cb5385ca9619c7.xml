        <script><![CDATA[var gFormAlreadySubmitted = false;
var _dialog = null;
var projectType = null;

function onSubmit() {
    if (gFormAlreadySubmitted || g_form.getValue("local_remote") != "Remote") {
        return true;
    }

	projectType = g_form.getValue("project_type");
	
	if (g_form.getValue("local_remote") == "Remote") {
		if (projectType == "Custom Application") {
			g_form.setValue("remote_update_sets", "");
		} else if (projectType == "Update Sets") {
			g_form.setValue("remote_application", "");
		}
	}
	
    var checkConnection =
        (projectType == "Custom application" && g_form.getValue("remote_application") == "") ||
        (projectType == "Update sets" && g_form.getValue("remote_update_sets") == "");

    if (checkConnection) {
        testRemoteConnection();
        return false;
    }

    return true;
}

function testRemoteConnection() {
    _dialog = new GlideModal("x_58056_md_docs_handle_remote_connection");
    _dialog.setTitle(new GwtMessage().getMessage("Remote Instance Connection"));
    _dialog.setPreference("sysID", g_form.getUniqueValue());
    _dialog.setPreference("instanceURL", g_form.getValue("remote_instance_url"));
    _dialog.setPreference("userName", g_form.getValue("remote_username"));
    _dialog.setPreference("password", g_form.getValue("remote_password"));
    _dialog.setPreference("projectType", projectType);
    _dialog.setPreference("connectionFail", connectionFail.bind(this));
    _dialog.setPreference("fetchSuccess", fetchSuccess.bind(this));
    _dialog.setPreference("fetchCancel", fetchCancel.bind(this));

    _dialog.render();
}

function connectionFail(msg) {
	g_form.clearMessages();
	_dialog.destroy();
	g_form.addErrorMessage(msg);
}

function fetchSuccess(remoteDetails) {
    _dialog.destroy();

    g_form.setValue("remote_json", JSON.stringify(remoteDetails));
    if (projectType == "Custom application") {
        g_form.setValue("remote_application", remoteDetails[0].display);
        g_form.setValue("remote_update_sets", "");
    } else {
        var updSets = [];
        remoteDetails.forEach(function(remoteDetail) {
            updSets.push(remoteDetail.display);
        }, this);
        g_form.setValue("remote_update_sets", updSets.join(","));
        g_form.setValue("remote_application", "");
    }
    submitForm();
}

function fetchCancel() {
    _dialog.destroy();
}

function submitForm() {
    gFormAlreadySubmitted = true;
    gsftSubmit(undefined, undefined, g_form.getActionName());
}
]]></script>
