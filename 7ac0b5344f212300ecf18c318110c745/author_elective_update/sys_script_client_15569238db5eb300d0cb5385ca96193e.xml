        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (newValue === '') {
		return;
	}
	
	if (newValue == "Remote") {
		var url = g_form.getValue("remote_instance_url");
		if (url == null || url.length == 0) {
			g_form.setValue("remote_instance_url", "https://instance.service-now.com");
		}
	} else {
		g_form.setValue("remote_instance_url", "");
	}
}
